<etk w="100%" h="100%">
	<rect x="0" y="0" w="100%" h="100%" color="white"/>
	<rect x="50%" y="25%" w="25%" h="25%" color="red">
		<anim name="x" from="10%" to="80%" calc="1" begin="mouseover" repeat="-1"/>
		<anim dur="8s" name="color" from="red" to="white" calc="1" begin="click" repeat="-1"/>
	</rect>
</etk>
